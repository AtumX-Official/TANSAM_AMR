<launch>

<rosparam file="$(find tansam_amr2_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="tansam_amr2" args="Revolute_right_position_controller Revolute_left_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/tansam_amr2/joint_states"/>
</node>

</launch>